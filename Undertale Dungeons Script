    game:service'ReplicatedStorage'.UseSpell:FireServer("Flowey Nukes")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("tEm BoMb")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Toxins")
end)

TextButton_3.Parent = ScrollingFrame
TextButton_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
