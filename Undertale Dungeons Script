TextLabel_2.TextSize = 14.000

TextButton_9.Parent = ScrollingFrame
TextButton_9.BackgroundColor3 = Color3.fromRGB(178,34,34)
TextButton_9.Position = UDim2.new(0.00200000009, 0, 0.24000000, 0)
TextButton_9.Size = UDim2.new(0, 184, 0, 64)
TextButton_9.Font = Enum.Font.SourceSans
TextButton_9.Text = "Destroy GUI"
TextButton_9.TextColor3 = Color3.fromRGB(0,0,0)
TextButton_9.TextSize = 14.000
TextButton_9.MouseButton1Down:connect(function()
    game:GetService("CoreGui").ScreenGui:Destroy()
end)

local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
Mouse.KeyDown:connect(function(Amogus)
local KeyToToggleScript = Amogus:lower()
if KeyToToggleScript == "f" then 
     game:service'ReplicatedStorage'.UseSpell:FireServer("Endless Stabs")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Knife Jabs")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Knife Throw")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Electric-Sword")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Magma-Sword")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Devilsknife")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Devestating Slash")
wait(2)
    game:service'ReplicatedStorage'.UseSpell:FireServer("Mana Lash")
end
end)

local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
Mouse.KeyDown:connect(function(Amogus)
local KeyToToggleScript = Amogus:lower()
